#processor block decomposition
sms(3,4,4)
#Always specify blocks in block base numbering
blk(on,0:2,0:3,0:3)

# tag boundary faces
tag("xMinFaces",face,(0:0,0:4,0:4))
tag("xMaxFaces",face,(3:3,0:4,0:4))
tag("yMinFaces",face,(0:3,0:0,0:4))
tag("yMaxFaces",face,(0:3,4:4,0:4))
tag("zMinFaces",face,(0:3,0:4,0:0))
tag("zMaxFaces",face,(0:3,0:4,4:4))

# define number of zones in each axis
numzones(38,38,38)

#Hex subdivisions
sub(10%,0:2, 0:3, 0:3,(7,0,0,0)) #7 hex
seed(10)

