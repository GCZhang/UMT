#processor block decomposition
sms(6,6,6)
#Always specify blocks in block base numbering
blk(on,0:5,0:5,0:5)

# tag boundary faces
tag("xMinFaces",face,(0:0,0:6,0:6))
tag("xMaxFaces",face,(6:6,0:6,0:6))
tag("yMinFaces",face,(0:6,0:0,0:6))
tag("yMaxFaces",face,(0:6,6:6,0:6))
tag("zMinFaces",face,(0:6,0:6,0:0))
tag("zMaxFaces",face,(0:6,0:6,6:6))

# define number of zones in each axis
numzones(20,20,20)

#Hex subdivisions
sub(10%,0:5, 0:5, 0:5,(7,0,0,0)) #7 hex
seed(10)

